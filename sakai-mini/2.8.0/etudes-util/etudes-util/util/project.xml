<project>
	<pomVersion>3</pomVersion>
	<extend>../../../master/project.xml</extend>
	<name>etudes-util</name>
	<groupId>etudes</groupId>
	<id>etudes-util</id>
	<currentVersion>1.0</currentVersion>
	<organization>
